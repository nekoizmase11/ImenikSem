@model IEnumerable<ImenikSem.Prezentation.Web.Models.KontaktPrezentacioniModel>

@{
    ViewBag.Title = "NajcescePretrazivani";
}

    <h2>10 Najcesce pretrazivanih kontakata:</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Ime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Prezime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Broj)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Ime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Prezime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Broj)
        </td>
        <td>
            @Html.ActionLink("Izmeni", "Izmeni", new { id=item.Id }) |
            @Html.ActionLink("Obrisi", "Obrisi", new { id=item.Id })
        </td>
    </tr>
}

</table>
